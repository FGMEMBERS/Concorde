<?xml version="1.0"?>

<PropertyList>

 <name>Concorde-systems</name>
 <width>600</width>
 <height>400</height>
 <modal>false</modal>

 <text>
  <x>10</x>
  <y>370</y>
  <label>Concorde Systems</label>
 </text>

 <text>
  <x>30</x>
  <y>340</y>
  <label>(Uncheck a system to fail it.)</label>
  <color>
    <red>0.5</red>
    <green>0.5</green>
    <blue>0.5</blue>
  </color>
 </text>

 <group>
  <y>270</y>

  <checkbox>
   <x>30</x>
   <y>0</y>
   <width>20</width>
   <height>20</height>
   <label>Air Bleed</label>
   <property>/systems/air-bleed/serviceable</property>
  </checkbox>

  <checkbox>
   <x>230</x>
   <y>0</y>
   <width>20</width>
   <height>20</height>
   <label>Air Conditioning</label>
   <property>/systems/temperature/serviceable</property>
  </checkbox>

  <checkbox>
   <x>430</x>
   <y>0</y>
   <width>20</width>
   <height>20</height>
   <label>Anti-icing</label>
   <property>/systems/anti-icing/serviceable</property>
  </checkbox>
 </group>

 <group>
  <y>240</y>

  <checkbox>
   <x>30</x>
   <y>0</y>
   <width>20</width>
   <height>20</height>
   <label>Autopilot 1</label>
   <property>/systems/autopilot/serviceable[0]</property>
  </checkbox>

  <checkbox>
   <x>230</x>
   <y>0</y>
   <width>20</width>
   <height>20</height>
   <label>Autopilot 2</label>
   <property>/systems/autopilot/serviceable[1]</property>
  </checkbox>

  <checkbox>
   <x>430</x>
   <y>0</y>
   <width>20</width>
   <height>20</height>
   <label>Autothrottle 1</label>
   <property>/systems/autothrottle/serviceable[0]</property>
  </checkbox>
 </group>

 <group>
  <y>210</y>

  <checkbox>
   <x>30</x>
   <y>0</y>
   <width>20</width>
   <height>20</height>
   <label>Autothrottle 2</label>
   <property>/systems/autothrottle/serviceable[1]</property>
  </checkbox>

  <checkbox>
   <x>230</x>
   <y>0</y>
   <width>20</width>
   <height>20</height>
   <label>Crew</label>
   <property>/systems/crew/serviceable</property>
  </checkbox>

  <checkbox>
   <x>430</x>
   <y>0</y>
   <width>20</width>
   <height>20</height>
   <label>Electrical</label>
   <property>/systems/electrical/serviceable</property>
  </checkbox>
 </group>

 <group>
  <y>180</y>

  <checkbox>
   <x>30</x>
   <y>0</y>
   <width>20</width>
   <height>20</height>
   <label>Fuel</label>
   <property>/systems/fuel/serviceable</property>
  </checkbox>

  <checkbox>
   <x>230</x>
   <y>0</y>
   <width>20</width>
   <height>20</height>
   <label>GPWS</label>
   <property>/systems/gpws/serviceable</property>
  </checkbox>

  <checkbox>
   <x>430</x>
   <y>0</y>
   <width>20</width>
   <height>20</height>
   <label>Hydraulic</label>
   <property>/systems/hydraulic/serviceable</property>
  </checkbox>
 </group>

 <group>
  <y>150</y>

  <checkbox>
   <x>30</x>
   <y>0</y>
   <width>20</width>
   <height>20</height>
   <label>Lighting</label>
   <property>/systems/lighting/serviceable</property>
  </checkbox>

  <checkbox>
   <x>230</x>
   <y>0</y>
   <width>20</width>
   <height>20</height>
   <label>Pitot</label>
   <property>/systems/pitot/serviceable</property>
  </checkbox>

  <checkbox>
   <x>430</x>
   <y>0</y>
   <width>20</width>
   <height>20</height>
   <label>Pressurization</label>
   <property>/systems/pressurization/serviceable</property>
  </checkbox>
 </group>

 <group>
  <y>120</y>

  <checkbox>
   <x>30</x>
   <y>0</y>
   <width>20</width>
   <height>20</height>
   <label>Static</label>
   <property>/systems/static/serviceable</property>
  </checkbox>

  <checkbox>
   <x>230</x>
   <y>0</y>
   <width>20</width>
   <height>20</height>
   <label>Tank</label>
   <property>/systems/tank/serviceable</property>
  </checkbox>

  <checkbox>
   <x>430</x>
   <y>0</y>
   <width>20</width>
   <height>20</height>
   <label>Vacuum</label>
   <property>/systems/vacuum/serviceable</property>
  </checkbox>
 </group>

 <group>
  <y>10</y>

  <button>
   <x>40</x>
   <y>0</y>
   <legend>Apply</legend>
   <equal>true</equal>
   <binding>
    <command>dialog-apply</command>
   </binding>
  </button>

  <button>
   <x>280</x>
   <y>0</y>
   <legend>Close</legend>
   <default>true</default>
   <equal>true</equal>
   <key>Esc</key>
   <binding>
    <command>dialog-apply</command>
   </binding>
   <binding>
    <command>dialog-close</command>
   </binding>
  </button>

 </group>

</PropertyList>
