<?xml version="1.0"?>

<!-- - creates booleans (impossible with Nasal), which would otherwise be empty strings by default.
     - makes more readable the initialization state.

     - see model animation for gear times.
-->

<PropertyList>

  <audio>
   <comm n="0">
    <selected type="bool">true</selected>
    <standby type="bool">false</standby>
   </comm>
   <comm n="1">
    <selected type="bool">true</selected>
    <standby type="bool">false</standby>
   </comm>
   <crew>
    <captain>
     <adf n="0">
      <volume type="double">0.0</volume>
      <volume-push type="bool">false</volume-push>            <!-- knob disabled -->
     </adf>
     <adf n="1">
      <volume type="double">0.0</volume>
      <volume-push type="bool">false</volume-push>
     </adf>
     <comm n="0">
      <volume type="double">0.6</volume>                      <!-- FG default -->
      <volume-push type="bool">false</volume-push>
     </comm>
     <comm n="1">
      <volume type="double">0.6</volume>
      <volume-push type="bool">false</volume-push>
     </comm>
     <nav n="0">
      <volume type="double">0.0</volume>
      <volume-push type="bool">false</volume-push>
     </nav>
     <nav n="1">
      <volume type="double">0.0</volume>
      <volume-push type="bool">false</volume-push>
     </nav>
    </captain>
    <copilot>
     <adf n="0">
      <volume type="double">0.0</volume>
      <volume-push type="bool">false</volume-push>
     </adf>
     <adf n="1">
      <volume type="double">0.0</volume>
      <volume-push type="bool">false</volume-push>
     </adf>
     <comm n="0">
      <volume type="double">0.6</volume>
      <volume-push type="bool">false</volume-push>
     </comm>
     <comm n="1">
      <volume type="double">0.6</volume>
      <volume-push type="bool">false</volume-push>
     </comm>
     <nav n="0">
      <volume type="double">0.0</volume>
      <volume-push type="bool">false</volume-push>
     </nav>
     <nav n="1">
      <volume type="double">0.0</volume>
      <volume-push type="bool">false</volume-push>
     </nav>
    </copilot>
    <engineer>
     <adf n="0">
      <volume type="double">0.0</volume>
      <volume-push type="bool">false</volume-push>
     </adf>
     <adf n="1">
      <volume type="double">0.0</volume>
      <volume-push type="bool">false</volume-push>
     </adf>
     <comm n="0">
      <volume type="double">0.6</volume>
      <volume-push type="bool">false</volume-push>
     </comm>
     <comm n="1">
      <volume type="double">0.6</volume>
      <volume-push type="bool">false</volume-push>
     </comm>
     <nav n="0">
      <volume type="double">0.0</volume>
      <volume-push type="bool">false</volume-push>
     </nav>
     <nav n="1">
      <volume type="double">0.0</volume>
      <volume-push type="bool">false</volume-push>
     </nav>
    </engineer>
   </crew>
  </audio>

  <autoflight>
   <altitude></altitude>
   <autopilot n="0">
    <engage type="bool">false</engage>
    <heading-select type="double">0.0</heading-select>
    <heading-true-select type="double">0.0</heading-true-select>
    <track-push type="bool">false</track-push>                <!-- magnetic heading -->
   </autopilot>
   <autopilot n="1">
    <engage type="bool">false</engage>
    <heading-select type="double">0.0</heading-select>
    <heading-true-select type="double">0.0</heading-true-select>
    <track-push type="bool">false</track-push>                <!-- magnetic heading -->
   </autopilot>
   <autothrottle n="0">
    <engage type="bool">false</engage>
   </autothrottle>
   <autothrottle n="1">
    <engage type="bool">false</engage>
   </autothrottle>
   <datum>
    <altitude type="double">0.0</altitude>
    <speed type="double">0.0</speed>
   </datum>
   <flight-director n="0">
    <engage type="bool">false</engage>
   </flight-director>
   <flight-director n="1">
    <engage type="bool">false</engage>
   </flight-director>
   <heading></heading>
   <horizontal></horizontal>
   <real-nav type="bool">true</real-nav>                      <!-- NAV accurate until 0 ft -->
   <speed></speed>
   <speed2></speed2>
   <vertical></vertical>
   <vertical2></vertical2>
  </autoflight>

  <crew>
   <copilot type="bool">false</copilot>
   <engineer type="bool">false</engineer>
   <minimized-s>10.0</minimized-s>                            <!-- 0 if no time out -->
   <voice>
   <sound type="bool">true</sound>
   <text type="bool">false</text>                             <!-- debug without Festival -->
   </voice>
  </crew>

  <doors>
   <flight-deck>
    <opened type="bool">false</opened>
    <normal type="bool">true</normal>
   </flight-deck>
   <engineer-deck>
    <out type="bool">false</out>
   </engineer-deck>
  </doors>

  <electric>
   <ac>
    <generator1 type="bool">true</generator1>                 <!-- generator to main bus -->
    <generator2 type="bool">true</generator2>
    <generator3 type="bool">true</generator3>
    <generator4 type="bool">true</generator4>
    <ground type="bool">false</ground>                        <!-- ground service -->
    <gpb type="bool">false</gpb>                              <!-- ground power breaker -->
    <btb1 type="bool">true</btb1>                             <!-- bus tie breaker -->
    <btb-guard1 type="bool">true</btb-guard1>
    <btb2 type="bool">true</btb2>
    <btb-guard2 type="bool">true</btb-guard2>
    <btb3 type="bool">true</btb3>
    <btb-guard3 type="bool">true</btb-guard3>
    <btb4 type="bool">true</btb4>
    <btb-guard4 type="bool">true</btb-guard4>
    <ssb type="bool">true</ssb>                               <!-- split system breaker -->
    <essential1 type="bool">true</essential1>                 <!-- main to essential -->
    <essential2 type="bool">true</essential2>
    <essential3 type="bool">true</essential3>
    <essential4 type="bool">true</essential4>
    <relight1 type="bool">true</relight1>                     <!-- relight to main bus -->
    <relight2 type="bool">true</relight2>
    <relight3 type="bool">true</relight3>
    <relight4 type="bool">true</relight4>
    <standby type="bool">true</standby>                       <!-- standby to AC essential -->
    <voltmeter type="int">0</voltmeter>                       <!-- vertical -->
    <emergency>
     <arm type="bool">true</arm>                              <!-- emergency generator armed -->
     <arm-guard type="bool">true</arm-guard>
     <generator type="bool">false</generator>                 <!-- generator to emergency bus -->
     <manual type="bool">false</manual>
     <asb type="bool">true</asb>                              <!-- auto shed breaker -->
     <essential1 type="bool">false</essential1>               <!-- emergency bus to AC essential -->
     <essential2 type="bool">false</essential2>
     <essential3 type="bool">false</essential3>
     <essential4 type="bool">false</essential4>
     <relight-selector type="int">0</relight-selector>        <!-- off -->
     <relight1 type="bool">false</relight1>                   <!-- emergency to relight -->
     <relight2 type="bool">false</relight2>
     <relight3 type="bool">false</relight3>
     <relight4 type="bool">false</relight4>
     <standby type="bool">false</standby>                     <!-- standby to DC essential (RAT not deployed) -->
    </emergency>
   </ac>
   <dc>
    <master-bat type="bool">true</master-bat>                 <!-- battery B -->
    <maina type="bool">true</maina>                           <!-- AC essential to DC main (TRU) -->
    <mainb type="bool">true</mainb>
    <essentiala type="bool">true</essentiala>                 <!-- AC essential to DC essential (TRU) -->
    <essentialb type="bool">true</essentialb>
    <emsa type="bool">true</emsa>                             <!-- essential / main split -->
    <emsb type="bool">true</emsb>
    <voltmeter type="int">0</voltmeter>                       <!-- vertical -->
   </dc>
  </electric>

  <engines>
   <engine n="0">
    <bug_pph>10000.0</bug_pph>
    <contingency type="bool">false</contingency>
    <csd type="bool">true</csd>
    <csd-guard type="bool">true</csd-guard>
    <master-bat type="bool">true</master-bat>                 <!-- battery A (like the old C++ electrical system) -->
    <master-alt type="bool">true</master-alt>                 <!-- generator -->
    <rating>takeoff</rating>
    <rating-flight>climb</rating-flight>
    <secondary-air-door type="bool">true</secondary-air-door>
    <throttle-manual>0.0</throttle-manual>                    <!-- differentiate autothrottle -->
   </engine>
   <engine n="1">
    <bug_pph>10000.0</bug_pph>
    <contingency type="bool">false</contingency>
    <csd type="bool">true</csd>
    <csd-guard type="bool">true</csd-guard>
    <master-alt type="bool">true</master-alt>                 <!-- generator -->
    <rating>takeoff</rating>
    <rating-flight>climb</rating-flight>
    <secondary-air-door type="bool">true</secondary-air-door>
    <throttle-manual>0.0</throttle-manual>
   </engine>
   <engine n="2">
    <bug_pph>10000.0</bug_pph>
    <contingency type="bool">false</contingency>
    <csd type="bool">true</csd>
    <csd-guard type="bool">true</csd-guard>
    <master-alt type="bool">true</master-alt>                 <!-- generator -->
    <rating>takeoff</rating>
    <rating-flight>climb</rating-flight>
    <secondary-air-door type="bool">true</secondary-air-door>
    <throttle-manual>0.0</throttle-manual>
   </engine>
   <engine n="3">
    <bug_pph>10000.0</bug_pph>
    <contingency type="bool">false</contingency>
    <csd type="bool">true</csd>
    <csd-guard type="bool">true</csd-guard>
    <master-alt type="bool">true</master-alt>                 <!-- generator -->
    <n1-to-limiter type="bool">true</n1-to-limiter>           <!-- 88% -->
    <rating>takeoff</rating>
    <rating-flight>climb</rating-flight>
    <secondary-air-door type="bool">true</secondary-air-door>
    <throttle-manual>0.0</throttle-manual>
   </engine>
  </engines>

  <fuel>
   <pumps>
    <engine type="bool">false</engine>
    <forward type="bool">false</forward>
    <aft type="bool">false</aft>
    <dump type="bool">false</dump>
    <dump2 type="bool">false</dump2>
    <cross type="bool">false</cross>
   </pumps>

   <tank n="0">
    <pump n="0" type="bool">true</pump>
    <pump n="1" type="bool">true</pump>
    <pump n="2" type="bool">true</pump>
    <aft-trim type="bool">false</aft-trim>
   </tank>
   <tank n="1">
    <pump n="0" type="bool">true</pump>
    <pump n="1" type="bool">true</pump>
    <pump n="2" type="bool">true</pump>
    <aft-trim type="bool">false</aft-trim>                    <!-- always -->
   </tank>
   <tank n="2">
    <pump n="0" type="bool">true</pump>
    <pump n="1" type="bool">true</pump>
    <pump n="2" type="bool">true</pump>
    <aft-trim type="bool">false</aft-trim>                    <!-- always -->
   </tank>
   <tank n="3">
    <pump n="0" type="bool">true</pump>
    <pump n="1" type="bool">true</pump>
    <pump n="2" type="bool">true</pump>
    <aft-trim type="bool">false</aft-trim>
   </tank>
   <tank n="4">
    <pump n="0" type="bool">true</pump>                       <!-- 5 to 1 -->
    <pump n="1" type="bool">true</pump>                       <!-- 5 to 2 -->
   </tank>
   <tank n="5">
    <pump n="0" type="bool">true</pump>                       <!-- 6 to 1 -->
    <pump n="1" type="bool">true</pump>                       <!-- 6 to 2 -->
   </tank>
   <tank n="6">
    <pump n="0" type="bool">true</pump>                       <!-- 7 to 3 -->
    <pump n="1" type="bool">true</pump>                       <!-- 7 to 4 -->
   </tank>
   <tank n="7">
    <pump n="0" type="bool">true</pump>                       <!-- 8 to 3 -->
    <pump n="1" type="bool">true</pump>                       <!-- 8 to 4 -->
   </tank>
   <tank n="8">
    <pump n="0" type="bool">true</pump>
    <pump n="1" type="bool">true</pump>
   </tank>
   <tank n="9">
    <pump n="0" type="bool">true</pump>
    <pump n="1" type="bool">true</pump>
   </tank>
   <tank n="10">
    <pump n="0" type="bool">true</pump>
    <pump n="1" type="bool">true</pump>
   </tank>
   <tank n="11">      <!-- 5A -->
    <pump n="0" type="bool">true</pump>
    <pump n="1" type="bool">true</pump>
    <trans-valve type="bool">false</trans-valve>              <!-- not connected -->
   </tank>
   <tank n="12">      <!-- 7A -->
    <pump n="0" type="bool">true</pump>
    <pump n="1" type="bool">true</pump>
    <trans-valve type="bool">false</trans-valve>              <!-- not connected -->
   </tank>
  </fuel>

  <gear>
   <brake-emergency type="bool">false</brake-emergency>       <!-- normal brakes -->
   <brake-parking-lever type="bool">false</brake-parking-lever>
   <brake-test type="bool">false</brake-test>
   <neutral type="bool">false</neutral>
   <time>
    <main>    
     <door-opened>0.17</door-opened>
     <lower-gear>0.17</lower-gear>
     <gear-locked>0.83</gear-locked>
     <close-door>0.83</close-door>
    </main>    
    <tail>    
     <open-door>0.17</open-door>
     <close-door>0.50</close-door>
     <gear-locked>0.83</gear-locked>
    </tail>    
   </time>
  </gear>

  <hydraulic>
   <circuits>
    <circuit n="0">
     <onloada type="bool">true</onloada>
     <shuta type="bool">false</shuta>
     <guarda type="bool">true</guarda>
     <onloadb type="bool">true</onloadb>
     <shutb type="bool">false</shutb>
     <guardb type="bool">true</guardb>
    </circuit>
    <circuit n="1">
     <onloada type="bool">true</onloada>
     <shuta type="bool">false</shuta>
     <guarda type="bool">true</guarda>
     <onloadb type="bool">true</onloadb>
     <shutb type="bool">false</shutb>
     <guardb type="bool">true</guardb>
    </circuit>
    <circuit n="2">
     <onloada type="bool">true</onloada>
     <shuta type="bool">false</shuta>
     <guarda type="bool">true</guarda>
     <onloadb type="bool">true</onloadb>
     <shutb type="bool">false</shutb>
     <guardb type="bool">true</guardb>
    </circuit>
   </circuits>
   <ground>
    <selector type="int">4</selector>                         <!-- green yellow -->
    <pump n="0">
     <switch>false</switch>
     <num type="int">0</num>
    </pump>
    <pump n="1">
     <switch>false</switch>
     <num type="int">1</num>
    </pump>
   </ground>
  </hydraulic>

  <lighting>
   <crew>
    <autoflight-norm type="double">1.0</autoflight-norm>
    <captain>
     <flood-light type="double">0.0</flood-light>             <!-- the real light -->
     <flood-norm type="double">0.0</flood-norm>               <!-- the knob -->
    </captain>
    <center>
     <flood-light type="double">0.0</flood-light>
     <flood-norm type="double">0.0</flood-norm>
    </center>
    <copilot>
     <flood-light type="double">0.0</flood-light>
     <flood-norm type="double">0.0</flood-norm>
    </copilot>
    <engineer>
     <flood-light type="double">0.0</flood-light>
     <flood-norm type="double">0.0</flood-norm>
    </engineer>
    <roof type="bool">false</roof>
    <roof-light type="double">0.0</roof-light>
    <roof-norm type="double">0.0</roof-norm>
   </crew>
   <instrument-lights type="bool">true</instrument-lights>    <!-- night lighting -->
   <external>
    <interpolate>                                             <!-- Nasal interpolate may not stop at 1.0 (bug ?) -->
     <off type="double">0.1</off>
     <on type="double">0.9</on>
    </interpolate>
    <landing-taxi n="0">
     <on type="bool">false</on>
     <extend type="bool">false</extend>
     <norm type="double">0.0</norm>
     <num type="int">0</num>
    </landing-taxi>
    <landing-taxi n="1">
     <on type="bool">false</on>
     <extend type="bool">false</extend>
     <norm type="double">0.0</norm>
     <num type="int">1</num>
    </landing-taxi>
    <main-landing n="0">
     <on type="bool">false</on>
     <extend type="bool">false</extend>
     <norm type="double">0.0</norm>
     <num type="int">0</num>
    </main-landing>
    <main-landing n="1">
     <on type="bool">false</on>
     <extend type="bool">false</extend>
     <norm type="double">0.0</norm>
     <num type="int">1</num>
    </main-landing>
    <taxi-turn n="0">
     <on type="bool">false</on>
     <num type="int">0</num>
    </taxi-turn>
    <taxi-turn n="1">
     <on type="bool">false</on>
     <num type="int">1</num>
    </taxi-turn>
   </external>
   <nav-lights type="bool">false</nav-lights>
   <strobe type="bool">false</strobe>
  </lighting>

  <nose-visor>
   <pos-norm alias="/controls/flight/flaps"/>                 <!-- FDM uses flaps as nose -->
  </nose-visor>

  <pneumatic>
   <engine n="0">
    <bleed-valve type="bool">true</bleed-valve>               <!-- connected to engine -->
    <cross-bleed-valve type="bool">false</cross-bleed-valve>  <!-- no ground supply or adjacent engine -->
    <conditioning-valve type="bool">true</conditioning-valve> <!-- air conditioning -->
    <fuel-valve type="bool">true</fuel-valve>
    <fuel-valve-guard type="bool">true</fuel-valve-guard>
   </engine>
   <engine n="1">
    <bleed-valve type="bool">true</bleed-valve>
    <cross-bleed-valve type="bool">false</cross-bleed-valve>
    <conditioning-valve type="bool">true</conditioning-valve>
    <fuel-valve type="bool">true</fuel-valve>
    <fuel-valve-guard type="bool">true</fuel-valve-guard>
   </engine>
   <engine n="2">
    <bleed-valve type="bool">true</bleed-valve>
    <cross-bleed-valve type="bool">false</cross-bleed-valve>
    <conditioning-valve type="bool">true</conditioning-valve>
    <fuel-valve type="bool">true</fuel-valve>
    <fuel-valve-guard type="bool">true</fuel-valve-guard>
   </engine>
   <engine n="3">
    <bleed-valve type="bool">true</bleed-valve>
    <cross-bleed-valve type="bool">false</cross-bleed-valve>
    <conditioning-valve type="bool">true</conditioning-valve>
    <fuel-valve type="bool">true</fuel-valve>
    <fuel-valve-guard type="bool">true</fuel-valve-guard>
   </engine>
  </pneumatic>

  <pressurization>
   <emergency>
    <depressurization type="bool">false</depressurization>
    <guard type="bool">true</guard>
   </emergency>
  </pressurization>

  <seat>
   <all type="bool">false</all>
  </seat>

  <temperature>
   <group n="0">
    <guard type="bool">true</guard>
    <on type="bool">true</on>
    <temperature-selector>1.5</temperature-selector>
   </group>
   <group n="1">
    <guard type="bool">true</guard>
    <on type="bool">true</on>
    <temperature-selector>1.5</temperature-selector>
   </group>
   <group n="2">
    <guard type="bool">true</guard>
    <on type="bool">true</on>
    <temperature-selector>1.5</temperature-selector>
   </group>
   <group n="3">
    <temperature-selector>1.5</temperature-selector>
   </group>
  </temperature>

</PropertyList>
