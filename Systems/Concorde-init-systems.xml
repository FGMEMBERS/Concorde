     <component>/engines</component>
     <subcomponent>engine</subcomponent>
    </engine>
    <engine2>
     <component>/systems/engines</component>
     <subcomponent>engine</subcomponent>
    </engine2>
    <weight>/instrumentation/weight-switch</weight>
   </slave>

