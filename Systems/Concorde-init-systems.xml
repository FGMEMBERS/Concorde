<?xml version="1.0"?>

<!-- intialize systems :

     - avoid nil values for Nasal.
     - avoid string numbers (comparison doesn't work in XML instrument) for Nasal.
     - autopilot, engines and gear also to distinguish the specific additions.

     - <slave> is a real dependancy on instruments.
     - <noinstrument> is to built the system; or for fuzzy decision of virtual crew :
       visual estimation of altitude over ground, if failure of radio altimeter;
       speed implicitly known, during a subsonic cruise.


     Known problems :
     ==============
     - electrical system has a Nasal stack overflow, when above <iterations-forced>4</iterations-forced>
     (not necessary).
-->

<PropertyList>

  <air-bleed>
   <serviceable type="bool" archive="y">true</serviceable>

   <engine n="0">
    <bleed-psi type="double">0.0</bleed-psi>
    <conditioning-psi type="double">0.0</conditioning-psi>
    <cross-psi type="double">0.0</cross-psi>
    <jet-pump type="bool">true</jet-pump>
    <ram-air type="bool">true</ram-air>
   </engine>
   <engine n="1">
    <bleed-psi type="double">0.0</bleed-psi>
    <conditioning-psi type="double">0.0</conditioning-psi>
    <cross-psi type="double">0.0</cross-psi>
    <jet-pump type="bool">true</jet-pump>
    <ram-air type="bool">true</ram-air>
   </engine>
   <engine n="2">
    <bleed-psi type="double">0.0</bleed-psi>
    <conditioning-psi type="double">0.0</conditioning-psi>
    <cross-psi type="double">0.0</cross-psi>
    <jet-pump type="bool">true</jet-pump>
    <ram-air type="bool">true</ram-air>
   </engine>
   <engine n="3">
    <bleed-psi type="double">0.0</bleed-psi>
    <conditioning-psi type="double">0.0</conditioning-psi>
    <cross-psi type="double">0.0</cross-psi>
    <jet-pump type="bool">true</jet-pump>
    <ram-air type="bool">true</ram-air>
   </engine>

   <ground-service>
    <door type="bool">false</door>                                           <!-- door closed -->
    <pressure-psi type="double">0.0</pressure-psi>
   </ground-service>

   <noinstrument>
    <agl>/position/altitude-agl-ft</agl>
    <airspeed>/velocities/airspeed-kt</airspeed>
    <temperature>/environment/temperature-degc</temperature>
   </noinstrument>

   <pressurization type="bool">false</pressurization>

   <slave>
    <engine>
     <component>/engines</component>
     <subcomponent>engine</subcomponent>
    </engine>
    <gear>/gear/gear[0]</gear>
   </slave>
  </air-bleed>

  <anti-icing>
   <serviceable type="bool" archive="y">true</serviceable>

   <detection>
    <coverage type="string"></coverage>
    <duration-min type="int">0</duration-min>
    <icing type="bool">false</icing>
   </detection>

   <noinstrument>
    <altitude>/position/altitude-ft</altitude>
    <cloud>
     <component>/environment/clouds</component>
     <subcomponent>layer</subcomponent>
    </cloud>
    <temperature>/environment/temperature-degc</temperature>
   </noinstrument>

   <power>
    <engine n="0" type="bool">false</engine>
    <engine n="1" type="bool">false</engine>
    <engine n="2" type="bool">false</engine>
    <engine n="3" type="bool">false</engine>
    <wing type="bool">false</wing>
   </power>

   <slave>
    <electric>/systems/electrical/power</electric>
   </slave>

   <warning type="bool">false</warning>
  </anti-icing>

  <autopilot n="0">
   <serviceable type="bool" archive="y">true</serviceable>

   <altimeter>
    <light-min-ft type="double">999.0</light-min-ft>
    <light-max-ft type="double">999.0</light-max-ft>
    <target-min-ft type="double">999.0</target-min-ft>
    <target-max-ft type="double">999.0</target-max-ft>
   </altimeter>

   <noinstrument>
    <altitude>/position/altitude-ft</altitude>
    <magnetic>/orientation/heading-magnetic-deg</magnetic>
    <pitch>/orientation/pitch-deg</pitch>
    <roll>/orientation/roll-deg</roll>
   </noinstrument>

   <slave>
    <altimeter>
     <component>/instrumentation</component>
     <subcomponent>altimeter</subcomponent>
    </altimeter>
    <aoa>
     <component>/instrumentation</component>
     <subcomponent>accelerometer-aoa</subcomponent>
    </aoa>
    <attitude>
     <component>/instrumentation</component>
     <subcomponent>attitude-indicator</subcomponent>
    </attitude>
    <dme>/instrumentation/dme[0]</dme>
    <electric>/systems/electrical/power</electric>
    <hsi>
      <component>/instrumentation</component>
      <subcomponent>hsi</subcomponent>
    </hsi>
    <ins>
      <component>/instrumentation</component>
      <subcomponent>ins</subcomponent>
    </ins>
    <ivsi>
     <component>/instrumentation</component>
     <subcomponent>inst-vertical-speed-indicator</subcomponent>
    </ivsi>
    <mach>
     <component>/instrumentation</component>
     <subcomponent>mach-indicator</subcomponent>
    </mach>
    <nav>
      <component>/instrumentation</component>
      <subcomponent>nav</subcomponent>
    </nav>
    <radio-altimeter>
     <component>/instrumentation</component>
     <subcomponent>radio-altimeter</subcomponent>
    </radio-altimeter>
    <weight>/instrumentation/ac-weight</weight>
   </slave>

   <state>
    <failure n="0" type="bool">false</failure>
    <failure n="1" type="bool">false</failure>
    <land2 type="bool">false</land2>
    <land3 type="bool">false</land3>
    <virtual type="bool">false</virtual>                                     <!-- no copilot -->
    <waypoint-nm type="double">0.0</waypoint-nm>
   </state>
  </autopilot>

  <autopilot n="1">
   <serviceable type="bool" archive="y">true</serviceable>
  </autopilot>

  <autothrottle n="0">
   <serviceable type="bool" archive="y">true</serviceable>

   <airspeed>
    <light-min-kt type="double">999.0</light-min-kt>
    <light-max-kt type="double">999.0</light-max-kt>
   </airspeed>

   <slave>
    <altimeter>/instrumentation/altimeter[0]</altimeter>
    <asi>
     <component>/instrumentation</component>
     <subcomponent>airspeed-indicator</subcomponent>
    </asi>
    <electric>/systems/electrical/power</electric>
    <mach>
     <component>/instrumentation</component>
     <subcomponent>mach-indicator</subcomponent>
    </mach>
    <throttle>
     <component>/systems/throttles</component>
     <subcomponent>throttle</subcomponent>
    </throttle>
   </slave>
  </autothrottle>

  <autothrottle n="1">
   <serviceable type="bool" archive="y">true</serviceable>
  </autothrottle>

  <crew>
   <serviceable type="bool">true</serviceable>

   <copilot>
    <activ type="bool">false</activ>                                         <!-- yellow -->

    <noinstrument>
     <agl>/position/altitude-agl-ft</agl>
     <airspeed>/velocities/airspeed-kt</airspeed>
     <altitude>/position/altitude-ft</altitude>
     <mach>/velocities/mach</mach>
    </noinstrument>

    <slave>
     <anti-icing>/controls/anti-ice</anti-icing>
     <flaps>/sim/flaps</flaps>
     <gear>/gear/gear[0]/position-norm</gear>
     <gear-ctrl>/controls/gear</gear-ctrl>
     <lighting>/controls/lighting/external</lighting>
     <mws>/controls/mws</mws>
     <to-monitor>/instrumentation/takeoff-monitor</to-monitor>
     <voice>/systems/voice</voice>
    </slave>

    <state></state>
    <time></time>
   </copilot>

   <emergency type="bool">false</emergency>

   <engineer>
    <activ type="bool">false</activ>                                        <!-- yellow -->

    <cg>
     <aft type="bool">false</aft>
     <forward type="bool">false</forward>
    </cg>

    <navigation>
     <wp n="0">
      <fuel-kg type="double">-999.0</fuel-kg>                               <!-- estimated fuel -->
      <speed-fpm type="double">0.0</speed-fpm>                              <!-- to acquired altitude -->
     </wp>
     <wp n="1">
      <fuel-kg type="double">-999.0</fuel-kg>
      <speed-fpm type="double">0.0</speed-fpm>
     </wp>
     <wp n="2">                                                             <!-- last waypoint -->
      <fuel-kg type="double">-999.0</fuel-kg>
      <speed-fpm type="double">0.0</speed-fpm>                              <!-- to land -->
     </wp>
    </navigation>

    <noinstrument>
     <agl>/position/altitude-agl-ft</agl>
     <altitude>/position/altitude-ft</altitude>
     <mach>/velocities/mach</mach>
    </noinstrument>

    <slave>
     <cg>/instrumentation/cg</cg>
     <fuel>/instrumentation/fuel</fuel>
     <fuel2>/systems/fuel</fuel2>
     <fuel-consumed>
      <component>/instrumentation</component>
      <subcomponent>fuel-consumed</subcomponent>
     </fuel-consumed>
     <ins>/instrumentation/ins/computed</ins>
     <voice>/systems/voice</voice>
    </slave>

    <state></state>
    <time></time>
   </engineer>

   <minimized type="bool">true</minimized>
  </crew>

  <electrical>
   <serviceable type="bool" archive="y">true</serviceable>

   <ground-service>
    <door type="bool">false</door>                                           <!-- door closed -->
    <volts type="double">0.0</volts>
   </ground-service>

   <internal>
    <config include="Concorde-electrical.xml"/>
    <iterations type="int">0</iterations>
    <iterations-forced type="int">2</iterations-forced>                      <!-- for correct parallel voltages -->
   </internal>

   <noinstrument>
    <agl>/position/altitude-agl-ft</agl>
    <airspeed>/velocities/airspeed-kt</airspeed>
    <temperature>/environment/temperature-degc</temperature>
   </noinstrument>

   <power>
    <autopilot n="0" type="bool">false</autopilot>
    <autopilot n="1" type="bool">false</autopilot>
    <ground-service type="bool">false</ground-service>
    <specific type="bool">false</specific>
   </power>

   <slave>
