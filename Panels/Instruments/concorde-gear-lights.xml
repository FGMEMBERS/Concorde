   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>15</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>55</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>Title left short</name>
   <type>text</type>
   <point-size>11</point-size>
   <color>
    <red>0.0</red>
    <green>0.0</green>
    <blue>0.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>UP L</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>-2</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>48</offset>
    </transformation>
   </transformations>
  </layer>

