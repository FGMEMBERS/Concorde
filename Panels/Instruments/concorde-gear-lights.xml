<!--
Concorde gear lights, like the real one : http://www.concordesst.com/

The constants 0.05 and 0.95 match the animation of the model.

Light meaning :
- yellow : lock released or closed during the gear operation
 (shortening lock for main gear, and upper lock for nose gear).
- red : gear door is opened, or tail gear in motion.
- green : gear down and locked.

-->

<PropertyList>
 <name>gear lights</name>
 <w-base>110</w-base>
 <h-base>110</h-base>

 <layers>

<!-- ==== -->
<!-- LOCK -->
<!-- ==== -->

<!-- LEFT SHORT -->

 <layer>
  <name>left short light</name>
   <type>switch</type>
  <layer>
   <name>On</name>
   <condition>
    <and>
     <property>/controls/gear/gear-down</property>
     <greater-than>
      <property>/gear/gear[1]/position-norm</property>
      <value>0.0</value>
     </greater-than>
     <less-than>
      <property>/gear/gear[1]/position-norm</property>
      <value>0.95</value>
     </less-than>
    </and>
   </condition>
   <texture>
    <path>Aircraft/Concorde/Panels/Instruments/Textures/warning-lamps.rgb</path>
    <x1>0.35</x1>
    <y1>0.0</y1>
    <x2>0.40</x2>
    <y2>0.095</y2>
   </texture>
   <w>36</w>
   <h>26</h>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>-25</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>55</offset>
    </transformation>
   </transformations>
  </layer>
  <layer>
   <name>Off</name>
   <texture>
    <path>Aircraft/Concorde/Panels/Instruments/Textures/warning-lamps.rgb</path>
    <x1>0.0</x1>
    <y1>0.0</y1>
    <x2>0.25</x2>
    <y2>0.095</y2>
   </texture>
   <w>36</w>
   <h>26</h>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>-25</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>55</offset>
    </transformation>
   </transformations>
  </layer>
 </layer>

  <layer>
   <name>background left short</name>
   <texture>
    <path>Aircraft/Concorde/Panels/Instruments/Textures/concorde-yellow-warning.rgb</path>
   </texture>
   <w>36</w>
   <h>26</h>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>-25</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>55</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>Title left short</name>
   <type>text</type>
   <point-size>11</point-size>
   <color>
    <red>0.0</red>
    <green>0.0</green>
    <blue>0.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>L SH</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>-42</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>48</offset>
    </transformation>
   </transformations>
  </layer>

<!-- UPPER LOCKS -->

 <layer>
  <name>upper locks light</name>
   <type>switch</type>
  <layer>
   <name>On</name>
   <condition>
    <or>
     <and>
      <not>
       <property>/controls/gear/gear-down</property>
      </not>
      <greater-than>
       <property>/gear/gear[0]/position-norm</property>
       <value>0.05</value>
      </greater-than>
      <less-than>
       <property>/gear/gear[0]/position-norm</property>
       <value>0.95</value>
      </less-than>
     </and>
     <and>
      <property>/controls/gear/gear-down</property>
      <greater-than>
       <property>/gear/gear[0]/position-norm</property>
       <value>0.0</value>
      </greater-than>
      <less-than>
       <property>/gear/gear[0]/position-norm</property>
       <value>0.05</value>
      </less-than>
     </and>
    </or>
   </condition>
   <texture>
    <path>Aircraft/Concorde/Panels/Instruments/Textures/warning-lamps.rgb</path>
    <x1>0.35</x1>
    <y1>0.0</y1>
    <x2>0.40</x2>
    <y2>0.095</y2>
   </texture>
   <w>36</w>
   <h>26</h>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>15</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>55</offset>
    </transformation>
   </transformations>
  </layer>
  <layer>
   <name>Off</name>
   <texture>
    <path>Aircraft/Concorde/Panels/Instruments/Textures/warning-lamps.rgb</path>
