<?xml version="1.0"?>

<!-- -->

<PropertyList>

 <name>Dial NAV</name>
 <w-base>124</w-base>
 <h-base>40</h-base>

 <params>
  <frq-disp>/instrumentation/nav[0]/frequencies/selected-mhz</frq-disp>
  <frq-ctrl>/instrumentation/nav[0]/frequencies/selected-mhz</frq-ctrl>
 </params>

 <layers>

  <layer>
   <name>background</name>
   <texture>
    <path>Aircraft/Concorde/Panels/Instruments/Textures/concorde-black-bg.rgb</path>
   </texture>
    <w>104</w>
    <h>40</h>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>49</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>0</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>Incr 1</name>
   <type>text</type>
   <point-size>22</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>+</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>-3</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>-4</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>Decr 1</name>
   <type>text</type>
   <point-size>22</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>-</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>-1</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>-22</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>Incr 0.01</name>
   <type>text</type>
   <point-size>22</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>+</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>10</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>-4</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>Decr 0.01</name>
   <type>text</type>
   <point-size>22</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>-</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>12</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>-22</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>Title</name>
   <type>text</type>
   <point-size>16</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>NAV</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>27</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>17</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>NAV frequency display</name>
   <type>text</type>
   <font>led</font>
   <point-size>8</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>number-value</type>
     <property alias="../../../../../params/frq-disp"/>
     <format>%3.2f</format>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>32</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>-8</offset>
    </transformation>
   </transformations>
  </layer>

 </layers>

 <actions>

  <action>
   <name>Minus 1</name>
   <button>0</button>
   <x>2</x>
   <y>-19</y>
   <w>14</w>
   <h>18</h>
   <binding>
    <command>property-adjust</command>
    <property alias="../../../../params/frq-ctrl"/>
    <step>-1</step>
    <min>110</min>
    <max>130</max>
    <wrap>130</wrap>
   </binding>
  </action>

  <action>
   <name>Plus 10</name>
   <button>0</button>
   <x>2</x>
   <y>1</y>
   <w>14</w>
   <h>18</h>
   <binding>
    <command>property-adjust</command>
    <property alias="../../../../params/frq-ctrl"/>
    <step>1</step>
    <min>110</min>
    <max>130</max>
    <wrap>110</wrap>
   </binding>
  </action>

  <action>
   <name>Minus 0.01</name>
   <button>0</button>
   <x>16</x>
   <y>-19</y>
   <w>14</w>
   <h>18</h>
   <binding>
    <command>property-adjust</command>
    <property alias="../../../../params/frq-ctrl"/>
    <step>-0.01</step>
    <min>110</min>
    <max>130</max>
    <wrap>130</wrap>
   </binding>
  </action>

  <action>
   <name>Plus 0.01</name>
   <button>0</button>
   <x>16</x>
   <y>1</y>
   <w>14</w>
   <h>18</h>
   <binding>
    <command>property-adjust</command>
    <property alias="../../../../params/frq-ctrl"/>
    <step>0.01</step>
    <min>110</min>
    <max>130</max>
    <wrap>110</wrap>
   </binding>
  </action>

 </actions>

</PropertyList>
