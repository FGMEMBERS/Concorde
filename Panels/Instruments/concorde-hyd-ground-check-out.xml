<?xml version="1.0"?>

<!-- Concorde ground hydraulics check out.

-->

<PropertyList>
 <name>Ground hydraulics check out</name>
 <w-base>216</w-base>
 <h-base>162</h-base>


 <layers>

<!-- YELLOW YELLOW -->

  <layer>
   <name>Title YY</name>
   <type>text</type>
   <point-size>8</point-size>
   <color>
