    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>YELLOW</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>30</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>-64</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>knob rotation</name>
   <texture>
    <path>Aircraft/Concorde/Panels/Instruments/Textures/navcom-radios.rgb</path>
    <x1>0.53125</x1>
    <x2>0.59765625</x2>
    <y1>0.05859375</y1>
    <y2>0.1484375</y2>
   </texture>
   <w>48</w>
   <h>60</h>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>0</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>-14</offset>
    </transformation>
    <transformation>
     <type>rotation</type>
     <condition>
      <less-than>
       <property>/controls/hydraulic/ground/selector</property>
       <value>3</value>
      </less-than>
     </condition>
     <property>/controls/hydraulic/ground/selector</property>
     <scale>60</scale>
     <offset>-150</offset>
    </transformation>
    <transformation>
     <type>rotation</type>
     <condition>
      <greater-than>
       <property>/controls/hydraulic/ground/selector</property>
       <value>2</value>
      </greater-than>
     </condition>
     <property>/controls/hydraulic/ground/selector</property>
     <scale>60</scale>
     <offset>-150</offset>
    </transformation>
    <transformation>
    <type>y-shift</type>
    <offset>-4</offset>
    </transformation>
   </transformations>
  </layer>

 </layers>

 <actions>

  <action>
   <name>selector left</name>
   <button>0</button>
   <x>-50</x>
   <y>-22</y>
   <w>50</w>
   <h>50</h>
   <binding>
    <command>property-adjust</command>
    <property>/controls/hydraulic/ground/selector</property>
    <step>-1</step>
    <min>0</min>
    <max>5</max>
    <wrap>0</wrap>
   </binding>
   <binding>
    <command>property-toggle</command>
    <property>/instrumentation/generic/click</property>
   </binding>
  </action>

  <action>
   <name>selector right</name>
   <button>0</button>
   <x>0</x>
   <y>-22</y>
   <w>50</w>
   <h>50</h>
   <binding>
    <command>property-adjust</command>
    <property>/controls/hydraulic/ground/selector</property>
    <step>1</step>
    <min>0</min>
    <max>5</max>
    <wrap>0</wrap>
   </binding>
   <binding>
    <command>property-toggle</command>
    <property>/instrumentation/generic/click</property>
   </binding>
  </action>

 </actions>

</PropertyList>
