<?xml version="1.0"?>

<!-- Alarm on tank pressure :

     - below -1.75 PSI (amber).
     - above 4.0 PSI (amber).
     - above 1.5 PSI (yellow) and raising.
     - below -0.8 PSI (yellow) and faling.
     - light is off.
-->

<PropertyList>

  <and>
   <greater-than>
    <property>/instrumentation/tank-pressure/differential-psi</property>
    <value>-1.75</value>
   </greater-than>
   <or>
    <greater-than>
     <property>/instrumentation/tank-pressure/differential-psi</property>
     <value>-0.8</value>
    </greater-than>
    <not>
     <property>/instrumentation/tank-pressure/falling</property>
    </not>
   </or>
   <or>
    <less-than-equals>
     <property>/instrumentation/tank-pressure/differential-psi</property>
     <value>1.5</value>
    </less-than-equals>
    <not>
     <property>/instrumentation/tank-pressure/raising</property>
    </not>
   </or>
   <less-than-equals>
    <property>/instrumentation/tank-pressure/differential-psi</property>
    <value>4.0</value>
   </less-than-equals>
  </and>

</PropertyList>
