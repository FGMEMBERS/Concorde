<?xml version="1.0"?>

<!-- Warning on autoland -->

<PropertyList>
 <name>Autoland check</name>
 <w-base>104</w-base>
 <h-base>26</h-base>


 <layers>
<!-- translucent lens -->  
  <layer>
   <name>Warning light</name>
   <type>switch</type>
    <layer>
     <name>on</name>
     <condition>
      <or>
       <equals>
        <property>/instrumentation/test-landing/on</property>
        <value>1.0</value>
       </equals>
       <equals>
        <property>/autopilot/locks/vertical</property>
        <value>autoland</value>
       </equals>
      </or>
     </condition>
     <texture>
     <path>Aircraft/Concorde/Panels/Instruments/Textures/warning-lamps.rgb</path>
     <x1>0.31</x1>
     <y1>0.0</y1>
     <x2>0.44</x2>
     <y2>0.095</y2>
     </texture>
     <w>55</w>
     <h>26</h>
    </layer>
    <layer>
     <name>off</name>
     <texture>
     <path>Aircraft/Concorde/Panels/Instruments/Textures/warning-lamps.rgb</path>
     <x1>0.0</x1>
     <y1>0.0</y1>
     <x2>0.25</x2>
     <y2>0.095</y2>
     </texture>
     <w>55</w>
     <h>26</h>
   </layer>
  </layer>

  <layer>
   <name>background</name>
   <texture>
    <path>Aircraft/Concorde/Panels/Instruments/Textures/concorde-green-warning.rgb</path>
     <x1>0.7</x1>
     <y1>0.8</y1>
     <x2>1.0</x2>
     <y2>0.9</y2>
   </texture>
   <w>55</w>
   <h>26</h>
  </layer>

  <layer>
   <name>Title</name>
   <type>switch</type>
 
   <layer>
    <name>Title real</name>
    <condition>
     <not>
      <property>/sim/panel/visibility</property>
     </not>
    </condition>
    <type>text</type>
    <point-size>15</point-size>
    <color>
     <red>1.0</red>
     <green>1.0</green>
     <blue>1.0</blue>
    </color>
    <chunks>
     <chunk>
      <type>literal</type>
      <text>Land</text>
     </chunk>
    </chunks>
    <transformations>
     <transformation>
      <type>x-shift</type>
      <offset>-19</offset>
     </transformation>
     <transformation>
      <type>y-shift</type>
      <offset>-3</offset>
     </transformation>
    </transformations>
   </layer>

   <layer>
    <name>Title abbreviation</name>
    <type>text</type>
    <point-size>29</point-size>
    <color>
     <red>1.0</red>
     <green>1.0</green>
     <blue>1.0</blue>
    </color>
    <chunks>
     <chunk>
      <type>literal</type>
      <text>LA</text>
     </chunk>
    </chunks>
    <transformations>
     <transformation>
      <type>x-shift</type>
      <offset>-19</offset>
     </transformation>
     <transformation>
      <type>y-shift</type>
      <offset>-6</offset>
     </transformation>
    </transformations>
   </layer>
  </layer>

 </layers>

</PropertyList>
