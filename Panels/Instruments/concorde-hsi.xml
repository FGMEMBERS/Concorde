<?xml version="1.0"?>

<!-- Concorde HSI title -->

<PropertyList>
 <name>HSI tittle</name>
 <w-base>104</w-base>
 <h-base>26</h-base>


 <layers>

  <layer>
   <name>Title</name>
   <type>text</type>
   <point-size>26</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>MAG</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>-31</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>-8</offset>
    </transformation>
   </transformations>
  </layer>

 </layers>

</PropertyList>
