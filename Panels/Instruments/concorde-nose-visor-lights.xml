<?xml version="1.0"?>

<!--
-->

<PropertyList>

 <name>Nose/Visor lights</name>
 <w-base>30</w-base>
 <h-base>80</h-base>

 <layers>

 <layer>
  <name>transit light</name>
   <type>switch</type>
  <layer>
   <name>On</name>
   <condition>
    <or>
     <and>
      <greater-than>
       <property>/surface-positions/flap-pos-norm</property>
       <value>0.0</value>
      </greater-than>
      <less-than-equals>
       <property>/surface-positions/flap-pos-norm</property>
       <value>0.31</value>
      </less-than-equals>
     </and>
     <and>
      <greater-than>
       <property>/surface-positions/flap-pos-norm</property>
       <value>0.34</value>
      </greater-than>
      <or>
       <less-than-equals>
        <property>/surface-positions/flap-pos-norm</property>
        <value>0.65</value>
       </less-than-equals>
       <greater-than-equals>
        <property>/surface-positions/flap-pos-norm</property>
        <value>0.69</value>
       </greater-than-equals>
      </or>
      <less-than>
       <property>/surface-positions/flap-pos-norm</property>
       <value>1.0</value>
      </less-than>
     </and>
    </or>
   </condition>
   <texture>
    <path>Aircraft/Concorde/Panels/Instruments/Textures/warning-lamps.rgb</path>
    <x1>0.34</x1>
    <y1>0.0</y1>
    <x2>0.41</x2>
    <y2>0.095</y2>
   </texture>
   <w>30</w>
   <h>30</h>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>0</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>60</offset>
    </transformation>
    <transformation>
     <type>rotation</type>
     <offset>90</offset>
    </transformation>
   </transformations>
  </layer>
  <layer>
   <name>Off</name>
   <condition>
    <and>
     <or>
      <equals>
       <property>/surface-positions/flap-pos-norm</property>
       <value>0.0</value>
      </equals>
      <greater-than>
       <property>/surface-positions/flap-pos-norm</property>
       <value>0.31</value>
      </greater-than>
     </or>
     <or>
      <less-than-equals>
       <property>/surface-positions/flap-pos-norm</property>
       <value>0.34</value>
      </less-than-equals>
      <and>
       <greater-than>
        <property>/surface-positions/flap-pos-norm</property>
        <value>0.65</value>
       </greater-than>
       <less-than>
        <property>/surface-positions/flap-pos-norm</property>
        <value>0.69</value>
       </less-than>
      </and>
      <equals>
       <property>/surface-positions/flap-pos-norm</property>
       <value>1.0</value>
      </equals>
     </or>
    </and>
   </condition>
   <texture>
    <path>Aircraft/Concorde/Panels/Instruments/Textures/warning-lamps.rgb</path>
    <x1>0.0</x1>
    <y1>0.0</y1>
    <x2>0.25</x2>
    <y2>0.095</y2>
   </texture>
   <w>30</w>
   <h>30</h>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>0</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>60</offset>
    </transformation>
    <transformation>
     <type>rotation</type>
     <offset>90</offset>
    </transformation>
   </transformations>
  </layer>
 </layer>

  <layer>
   <name>background transit</name>
   <texture>
    <path>Aircraft/Concorde/Panels/Instruments/Textures/concorde-red-alarm.rgb</path>
   </texture>
    <w>30</w>
    <h>30</h>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>0</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>60</offset>
    </transformation>
   </transformations>
  </layer>

 <layer>
  <name>5 deg light</name>
   <type>switch</type>
  <layer>
   <name>On</name>
   <condition>
    <and>
     <greater-than>
      <property>/surface-positions/flap-pos-norm</property>
      <value>0.65</value>
     </greater-than>
     <less-than>
      <property>/surface-positions/flap-pos-norm</property>
      <value>1.0</value>
     </less-than>
    </and>
   </condition>
   <texture>
    <path>Aircraft/Concorde/Panels/Instruments/Textures/warning-lamps.rgb</path>
    <x1>0.34</x1>
    <y1>0.0</y1>
    <x2>0.41</x2>
    <y2>0.095</y2>
   </texture>
   <w>30</w>
   <h>30</h>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>0</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>30</offset>
    </transformation>
    <transformation>
     <type>rotation</type>
     <offset>90</offset>
    </transformation>
   </transformations>
  </layer>
  <layer>
   <name>Off</name>
   <condition>
    <or>
     <less-than-equals>
      <property>/surface-positions/flap-pos-norm</property>
      <value>0.65</value>
     </less-than-equals>
     <greater-than-equals>
      <property>/surface-positions/flap-pos-norm</property>
      <value>1.0</value>
     </greater-than-equals>
    </or>
   </condition>
   <texture>
    <path>Aircraft/Concorde/Panels/Instruments/Textures/warning-lamps.rgb</path>
    <x1>0.0</x1>
    <y1>0.0</y1>
    <x2>0.25</x2>
    <y2>0.095</y2>
   </texture>
   <w>30</w>
   <h>30</h>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>0</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>30</offset>
    </transformation>
    <transformation>
     <type>rotation</type>
     <offset>90</offset>
    </transformation>
   </transformations>
  </layer>
 </layer>

  <layer>
   <name>background 5 deg</name>
   <texture>
    <path>Aircraft/Concorde/Panels/Instruments/Textures/concorde-yellow-warning.rgb</path>
   </texture>
   <w>30</w>
   <h>30</h>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>0</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>30</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>Title 5 deg</name>
   <type>text</type>
   <point-size>15</point-size>
   <color>
    <red>0.0</red>
    <green>0.0</green>
    <blue>0.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>5</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>-4</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>25</offset>
    </transformation>
   </transformations>
  </layer>

 <layer>
  <name>down light</name>
   <type>switch</type>
  <layer>
   <name>On</name>
   <condition>
    <equals>
     <property>/surface-positions/flap-pos-norm</property>
     <value>1.0</value>
    </equals>
   </condition>
   <texture>
    <path>Aircraft/Concorde/Panels/Instruments/Textures/warning-lamps.rgb</path>
    <x1>0.34</x1>
    <y1>0.0</y1>
    <x2>0.41</x2>
    <y2>0.095</y2>
   </texture>
   <w>30</w>
   <h>30</h>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>0</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>0</offset>
    </transformation>
    <transformation>
     <type>rotation</type>
     <offset>90</offset>
    </transformation>
   </transformations>
  </layer>
  <layer>
   <name>Off</name>
   <condition>
    <not-equals>
     <property>/surface-positions/flap-pos-norm</property>
     <value>1.0</value>
    </not-equals>
   </condition>
   <texture>
    <path>Aircraft/Concorde/Panels/Instruments/Textures/warning-lamps.rgb</path>
    <x1>0.0</x1>
    <y1>0.0</y1>
    <x2>0.25</x2>
    <y2>0.095</y2>
   </texture>
   <w>30</w>
   <h>30</h>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>0</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>0</offset>
    </transformation>
    <transformation>
     <type>rotation</type>
     <offset>90</offset>
    </transformation>
   </transformations>
  </layer>
 </layer>

  <layer>
   <name>background down</name>
   <texture>
    <path>Aircraft/Concorde/Panels/Instruments/Textures/concorde-green-warning.rgb</path>
   </texture>
   <w>30</w>
   <h>30</h>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>0</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>0</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>Title down</name>
   <type>text</type>
   <point-size>15</point-size>
   <color>
    <red>0.0</red>
    <green>0.0</green>
    <blue>0.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>V</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>-4</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>-7</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>Title arrow</name>
   <type>text</type>
   <point-size>15</point-size>
   <color>
    <red>0.0</red>
    <green>0.0</green>
    <blue>0.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>I</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>-1</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>-3</offset>
    </transformation>
   </transformations>
  </layer>

 </layers>

</PropertyList>
