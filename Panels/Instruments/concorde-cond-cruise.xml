<?xml version="1.0"?>

<!-- Engine rating cruise. 
-->

<PropertyList>

 <or>
  <and>
   <equals>
    <property>/autopilot/locks/altitude</property>
    <value>altitude-hold</value>
   </equals>
   <greater-than>
    <property>/instrumentation/altimeter/indicated-altitude-ft</property>
    <property n="1">/instrumentation/altimeter/light-min-ft</property>
   </greater-than>
   <less-than>
    <property>/instrumentation/altimeter/indicated-altitude-ft</property>
    <property n="1">/instrumentation/altimeter/light-max-ft</property>
   </less-than>
   <not include="concorde-cond-takeoff.xml">
   </not>
  </and>
  <greater-than-equals>
   <property>/velocities/mach</property>
   <value>1.95</value> <!-- see check-list -->
  </greater-than-equals>
 </or>

</PropertyList>
