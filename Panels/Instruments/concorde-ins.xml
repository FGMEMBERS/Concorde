<?xml version="1.0"?>

<!-- Concorde Inertial Navigation System

The LED is real (with probably different display).

-->

<PropertyList>
 <name>INS Computer</name>
 <w-base>108</w-base>
 <h-base>48</h-base>

 <params>
  <on-ctrl>/instrumentation/ins/on</on-ctrl>
  <selector-ctrl>/instrumentation/ins/selector</selector-ctrl>
  <wpt-ctrl>/instrumentation/ins/waypoint</wpt-ctrl>
 </params>


 <layers>

<!-- translucent lens (partially used) -->  
  <layer>
   <name>darken background left</name>
   <type>switch</type>
    <layer>
     <name>on</name>
     <condition>
      <equals>
       <property>/autopilot/route-manager/wp/id</property>
       <value>NOT-USED</value>
      </equals>
     </condition>
     <texture>
     <path>Aircraft/Concorde/Panels/Instruments/Textures/warning-lamps.rgb</path>
     <x1>0.25</x1>
     <y1>0.0</y1>
     <x2>0.5</x2>
     <y2>0.095</y2>
     </texture>
     <w>100</w>
     <h>46</h>
    </layer>
    <layer>
     <name>off</name>
     <texture>
     <path>Aircraft/Concorde/Panels/Instruments/Textures/warning-lamps.rgb</path>
     <x1>0.0</x1>
     <y1>0.0</y1>
     <x2>0.25</x2>
     <y2>0.095</y2>
     </texture>
     <w>100</w>
     <h>46</h>
   </layer>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>5</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>77</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>background left led</name>
   <texture>
    <path>Aircraft/Concorde/Panels/Instruments/Textures/concorde-green-warning.rgb</path>
     <x1>0.7</x1>
     <y1>0.8</y1>
     <x2>1.0</x2>
     <y2>0.9</y2>
   </texture>
   <w>100</w>
   <h>46</h>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>5</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>77</offset>
    </transformation>
   </transformations>
  </layer>

<!-- translucent lens (partially used) -->  
  <layer>
   <name>darken background right</name>
   <type>switch</type>
    <layer>
     <name>on</name>
     <condition>
      <equals>
       <property>/autopilot/route-manager/wp/id</property>
       <value>NOT-USED</value>
      </equals>
     </condition>
     <texture>
     <path>Aircraft/Concorde/Panels/Instruments/Textures/warning-lamps.rgb</path>
     <x1>0.25</x1>
     <y1>0.0</y1>
     <x2>0.5</x2>
     <y2>0.095</y2>
     </texture>
     <w>120</w>
     <h>46</h>
    </layer>
    <layer>
     <name>off</name>
     <texture>
     <path>Aircraft/Concorde/Panels/Instruments/Textures/warning-lamps.rgb</path>
     <x1>0.0</x1>
     <y1>0.0</y1>
     <x2>0.25</x2>
     <y2>0.095</y2>
     </texture>
     <w>120</w>
     <h>46</h>
   </layer>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>120</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>77</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>background right led</name>
   <texture>
    <path>Aircraft/Concorde/Panels/Instruments/Textures/concorde-green-warning.rgb</path>
     <x1>0.7</x1>
     <y1>0.8</y1>
     <x2>1.0</x2>
     <y2>0.9</y2>
   </texture>
   <w>120</w>
   <h>46</h>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>120</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>77</offset>
    </transformation>
   </transformations>
  </layer>

<!-- POSITION -->

  <layer>
   <name>Latitude</name>
   <condition>
    <and>
     <property alias="../../../../../params/on-ctrl"/>
     <or>
      <less-than>
       <property alias="../../../../../../../params/selector-ctrl"/>
       <value type="double">1</value>
      </less-than>
      <greater-than>
       <property alias="../../../../../../../params/selector-ctrl"/>
       <value type="double">3</value>
      </greater-than>
     </or>
    </and>
   </condition>
   <type>text</type>
   <font>led</font>
   <point-size>9</point-size>
   <color>
    <red>0.0</red>
    <green>0.85</green>
    <blue>0.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>number-value</type>
     <property>/position/latitude-deg</property>
     <format>%2.2f</format>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>-32</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>68</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>Longitude</name>
   <condition>
    <and>
     <property alias="../../../../../params/on-ctrl"/>
     <or>
      <less-than>
       <property alias="../../../../../../../params/selector-ctrl"/>
       <value type="double">1</value>
      </less-than>
      <greater-than>
       <property alias="../../../../../../../params/selector-ctrl"/>
       <value type="double">3</value>
      </greater-than>
     </or>
    </and>
   </condition>
   <type>text</type>
   <font>led</font>
   <point-size>9</point-size>
   <color>
    <red>0.0</red>
    <green>0.85</green>
    <blue>0.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>number-value</type>
     <property>/position/longitude-deg</property>
     <format>%3.2f</format>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>73</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>68</offset>
    </transformation>
   </transformations>
  </layer>

