<?xml version="1.0"?>

<!-- Condition (on) for gear up :

     - green hydraulics.
     - prevents retract on ground.
     - not neutral.
-->

<PropertyList>

  <and include="concorde-cond-voltage-gear.xml">
   <not include="concorde-cond-hyd-green.xml"/>
    <greater-than>
     <property>/instrumentation/radio-altimeter/indicated-altitude-ft</property>
     <value>20.0</value>
    </greater-than>
    <not>
     <property>/controls/gear/neutral</property>
    </not>
    <property>/controls/gear/gear-down</property>
  </and>

</PropertyList>
