<!-- LOD  -->
<!-- ==== -->

<!-- increases frame rate inside the cockpit, when looking backwards (parts above 1000 vertices) -->

  <animation>
   <type>select</type>
   <object-name>Hull</object-name>
   <object-name>Wing</object-name>
   <object-name>PrimaryNozzle</object-name>
   <object-name>NozzleTop1</object-name>
   <object-name>NozzleBottom1</object-name>
   <object-name>NozzleTop2</object-name>
   <object-name>NozzleBottom2</object-name>
   <object-name>NozzleTop3</object-name>
   <object-name>NozzleBottom3</object-name>
   <object-name>NozzleTop4</object-name>
   <object-name>NozzleBottom4</object-name>
   <object-name>Intake</object-name>
   <object-name>EngineIntake</object-name>
   <object-name>Engine</object-name>
   <condition>
    <not>
     <property>/sim/current-view/internal</property>
    </not>
   </condition>
  </animation>

<!-- a 2nd group, otherwise ignored (bug ?) -->

  <animation>
   <type>select</type>
   <object-name>Fwheels</object-name>
   <object-name>Fstrut</object-name>
   <object-name>Rmainwheels</object-name>
   <object-name>RMainstrut</object-name>
   <object-name>Lmainwheels</object-name>
   <object-name>LMainstrut</object-name>
   <object-name>Cabin</object-name>
   <condition>
    <not>
     <property>/sim/current-view/internal</property>
    </not>
   </condition>
  </animation>

<!-- increase frame rate in Captain view -->

  <animation>
   <type>select</type>
   <object-name>Nose</object-name>
   <object-name>Screen</object-name>
   <condition>
    <or>
     <not>
      <property>/sim/current-view/internal</property>
     </not>
     <less-than>
      <property>/surface-positions/flap-pos-norm</property>
      <value>1.0</value> <!-- not visible when down -->
     </less-than>
    </or>
   </condition>
  </animation>

<!-- ========== -->
<!-- Animations -->
<!-- ========== -->

<!-- NOSE -->

  <animation>
  <type>rotate</type>
  <object-name>Nose</object-name>
  <property>/surface-positions/flap-pos-norm</property>
  <interpolation> <!-- real angles -->
    <entry>
     <ind>0.0</ind>
     <dep>0</dep>
    </entry>
    <entry>
     <ind>0.34</ind>
     <dep>0</dep>
    </entry>
    <entry>
     <ind>0.68</ind>
     <dep>-5</dep>
    </entry>
    <entry>
     <ind>1.0</ind>
     <dep>-12.5</dep>
    </entry>
  </interpolation>
  <center>
   <x-m>-24.0</x-m>
   <y-m>0</y-m>
   <z-m>1.9</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

<!-- VISOR -->

<!-- lower the visor inside the nose (curved guide rails) -->

  <animation>
  <type>translate</type>
  <object-name>Screen</object-name>
  <property>/surface-positions/flap-pos-norm</property>
  <interpolation>
    <entry>
     <ind>0.0</ind>
     <dep>0</dep>
    </entry>
    <entry>
     <ind>0.06</ind>
     <dep>-0.02</dep>
    </entry>
    <entry>
     <ind>0.12</ind>
     <dep>-0.15</dep>
    </entry>
    <entry>
     <ind>0.18</ind>
     <dep>-0.25</dep>
    </entry>
    <entry>
     <ind>0.23</ind>
     <dep>-0.33</dep>
    </entry>
    <entry>
     <ind>0.29</ind>
     <dep>-0.38</dep>
    </entry>
    <entry>
     <ind>0.34</ind>
     <dep>-0.40</dep> <!-- down -->
    </entry>
    <entry>
     <ind>1.0</ind>
     <dep>-0.40</dep>
    </entry>
  </interpolation>
  <axis>
   <x>0</x>
   <y>0</y>
   <z>1</z>
  </axis>
 </animation>

  <animation>
  <type>translate</type>
  <object-name>Screen</object-name>
  <property>/surface-positions/flap-pos-norm</property>
  <interpolation>
    <entry>
     <ind>0.0</ind>
     <dep>0</dep>
    </entry>
    <entry>
     <ind>0.34</ind>
     <dep>-0.80</dep> <!-- forwards -->
    </entry>
    <entry>
     <ind>1.0</ind>
     <dep>-0.80</dep>
    </entry>
  </interpolation>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

