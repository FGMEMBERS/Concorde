  <type>rotate</type>
  <object-name>Tailwheels</object-name>
  <property>gear/gear[3]/position-norm</property>
  <factor>50</factor>
  <offset-deg>-50</offset-deg>
  <center>
   <x-m>22.9</x-m>
   <y-m>0.0</y-m>
   <z-m>-1.1</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>select</type>
  <object-name>Tailwheels</object-name> <!-- spare frame rate -->
  <condition>
   <greater-than>
    <property>/gear/gear[3]/position-norm</property>
    <value>0.0</value>
   </greater-than>
  </condition>
 </animation>

  <animation>
 <type>rotate</type>
  <object-name>Tailstrut</object-name>
  <property>gear/gear[3]/position-norm</property>
  <factor>50</factor>
  <offset-deg>-50</offset-deg>
  <center>
   <x-m>22.9</x-m>
   <y-m>0.0</y-m>
   <z-m>-1.1</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation> 

 <animation>
  <type>select</type>
  <object-name>Tailstrut</object-name> <!-- spare frame rate -->
  <condition>
   <greater-than>
    <property>/gear/gear[3]/position-norm</property>
    <value>0.0</value>
   </greater-than>
  </condition>
 </animation>

  <animation>
 <type>rotate</type>
  <object-name>Tailram</object-name>
  <property>gear/gear[3]/position-norm</property>
  <factor>50</factor>
  <offset-deg>-50</offset-deg>
  <center>
   <x-m>22.9</x-m> <!-- real axis is along the leg door -->
   <y-m>0.0</y-m>
   <z-m>-1.1</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation> 

 <animation>
  <type>select</type>
  <object-name>Tailram</object-name> <!-- spare frame rate -->
  <condition>
   <greater-than>
    <property>/gear/gear[3]/position-norm</property>
    <value>0.0</value>
   </greater-than>
  </condition>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>Littledoorleft.</object-name> <!-- small left door -->
  <property>gear/gear[3]/position-norm</property>
   <interpolation>
    <entry>
      <ind>0.0</ind>
      <dep>90.0</dep>
    </entry>
    <entry>
      <ind>0.05</ind>
      <dep>90.0</dep>
    </entry>
    <entry>
      <ind>0.15</ind>
      <dep>0.0</dep>
    </entry>
    <entry>
      <ind>1.0</ind>
      <dep>0.0</dep>
    </entry>
  </interpolation>   
  <center>
   <x-m>24.2</x-m>
   <y-m>-0.26</y-m>
   <z-m>-1.32</z-m>
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0.25</z> <!-- slope 1 for 4 -->
  </axis>
 </animation>

  <animation>
  <type>rotate</type>
  <object-name>Littledoorright</object-name> <!-- small right door -->
  <property>gear/gear[3]/position-norm</property>
   <interpolation>
    <entry>
      <ind>0.0</ind>
      <dep>-90.0</dep>
    </entry>
    <entry>
      <ind>0.05</ind>
      <dep>-90.0</dep>
    </entry>
    <entry>
      <ind>0.15</ind>
      <dep>0.0</dep>
    </entry>
    <entry>
      <ind>1.0</ind>
      <dep>0.0</dep>
    </entry>
  </interpolation>   
  <center>
   <x-m>24.2</x-m>
   <y-m>0.26</y-m>
   <z-m>-1.32</z-m>
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0.25</z> <!-- slope 1 for 4 -->
  </axis>
 </animation> 

  <animation>
  <type>rotate</type>
  <object-name>Taildoorfront</object-name> <!-- door behind the wheel -->
  <property>gear/gear[3]/position-norm</property>
   <interpolation>
    <entry>
      <ind>0.0</ind>
      <dep>-36.0</dep>
    </entry>
    <entry>
      <ind>0.05</ind>
      <dep>-36.0</dep>
    </entry>
    <entry>
      <ind>0.15</ind>
      <dep>0.0</dep>
    </entry>
    <entry>
      <ind>1.0</ind>
      <dep>0.0</dep>
    </entry>
  </interpolation>   
  <center>
   <x-m>22.95</x-m>
   <y-m>0.0</y-m>
   <z-m>-1.25</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation> 

