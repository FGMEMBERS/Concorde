<!-- FRONT GEAR -->

 <animation>
  <type>select</type>
  <object-name>Fwheels</object-name> <!-- spare frame rate -->
  <object-name>Fstrut</object-name>
  <object-name>Fhub</object-name>
  <condition>
   <greater-than>
    <property>/gear/gear[0]/position-norm</property>
    <value>0.0</value>
   </greater-than>
  </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>Fram</object-name> <!-- visible through door -->
  <condition>
