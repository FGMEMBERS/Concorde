<?xml version="1.0"?>
<PropertyList>
  <path>../Models/asi.ac</path>
  <animation>
    <type>rotate</type>
    <object-name>ASINeedle</object-name>
    <property>instrumentation/airspeed-indicator[0]/indicated-speed-kt</property>
    <interpolation>
      <entry>
        <ind>60</ind>
        <dep>-8</dep>
      </entry>
      <entry>
        <ind>200</ind>
        <dep>96</dep>
      </entry>
      <entry>
        <ind>250</ind>
        <dep>133</dep>
      </entry>
      <entry>
        <ind>350</ind>
        <dep>175</dep>
      </entry>
      <entry>
        <ind>570</ind>
        <dep>340</dep>
      </entry>
    </interpolation>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>LimitNeedle</object-name>
    <property>instrumentation/airspeed-indicator[0]/vmo-kt</property>
    <interpolation>
      <entry>
        <ind>60</ind>
        <dep>-8</dep>
      </entry>
      <entry>
        <ind>200</ind>
        <dep>96</dep>
      </entry>
      <entry>
        <ind>250</ind>
        <dep>133</dep>
      </entry>
      <entry>
        <ind>350</ind>
        <dep>175</dep>
      </entry>
      <entry>
        <ind>570</ind>
        <dep>340</dep>
      </entry>
    </interpolation>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>Number1</object-name>
    <expression>
      <prod>
        <sum>
          <floor>
            <div>
              <property>instrumentation/airspeed-indicator[0]/indicated-speed-kt</property>
              <value>100</value>
            </div>
          </floor>
          <max>
            <difference>
              <property>instrumentation/airspeed-indicator[0]/indicated-speed-kt</property>
              <value>99</value>
              <prod>
                <floor>
                  <div>
                    <property>instrumentation/airspeed-indicator[0]/indicated-speed-kt</property>
                    <value>100</value>
                  </div>
                </floor>
                <value>100</value>
              </prod>
            </difference>
            <value>0</value>
          </max>
        </sum>
        <value>36</value>
      </prod>
    </expression>
    <center>
      <x-m>-0.0085</x-m>
      <y-m>-0.004</y-m>
      <z-m>0.015</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>Number2</object-name>
    <expression>
      <prod>
        <sum>
          <floor>
            <div>
              <property>instrumentation/airspeed-indicator[0]/indicated-speed-kt</property>
              <value>10</value>
            </div>
          </floor>
          <max>
            <difference>
              <property>instrumentation/airspeed-indicator[0]/indicated-speed-kt</property>
              <value>9</value>
              <prod>
                <floor>
                  <div>
                    <property>instrumentation/airspeed-indicator[0]/indicated-speed-kt</property>
                    <value>10</value>
                  </div>
                </floor>
                <value>10</value>
              </prod>
            </difference>
            <value>0</value>
          </max>
        </sum>
        <value>36</value>
      </prod>
    </expression>
    <center>
      <x-m>-0.0085</x-m>
      <y-m>0</y-m>
      <z-m>0.015</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>Number3</object-name>
    <property>instrumentation/airspeed-indicator[0]/indicated-speed-kt</property>
    <factor>36</factor>
    <center>
      <x-m>-0.0085</x-m>
      <y-m>0.004</y-m>
      <z-m>0.015</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>
  <!--Default Lighting-->
  <animation>
    <type>material</type>
    <object-name>Number1</object-name>
    <object-name>Number2</object-name>
    <object-name>Number3</object-name>
    <object-name>LimitNeedle</object-name>
    <object-name>Guage</object-name>
    <emission>
      <red>0.2</red>
      <green>0.1</green>
      <blue>0.1</blue>
      <factor-prop>controls/lighting/panel-norm</factor-prop>
    </emission>
  </animation>
  <animation>
    <type>material</type>
    <object-name>Frame</object-name>
    <object-name>FrameC</object-name>
    <emission>
      <red>0.01</red>
      <green>0.005</green>
      <blue>0.005</blue>
      <factor-prop>controls/lighting/panel-norm</factor-prop>
    </emission>
  </animation>
  <animation>
    <type>material</type>
    <object-name>ASINeedle</object-name>
    <emission>
      <red>0.1</red>
      <green>0.03</green>
      <blue>0.03</blue>
      <factor-prop>controls/lighting/panel-norm</factor-prop>
    </emission>
  </animation>
</PropertyList>
