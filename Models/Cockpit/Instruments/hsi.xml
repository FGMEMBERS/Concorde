<?xml version="1.0"?>
<PropertyList>
  <path>../Models/hsi.ac</path>
  <animation>
    <type>rotate</type>
    <object-name>Compass</object-name>
    <property>/instrumentation/hsi[0]/heading-deg</property>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>-0.0125</z-m>
    </center>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>HdgBug</object-name>
    <expression>
      <difference>
        <property>/controls/autoflight/autopilot[0]/heading-select</property>
        <property>/instrumentation/hsi[0]/heading-deg</property>
      </difference>
    </expression>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>-0.0125</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>CompassNeedle</object-name>
    <object-name>LocGuage</object-name>
    <object-name>LocNeedle</object-name>
    <object-name>ToFlag</object-name>
    <object-name>FromFlag</object-name>
    <expression>
      <difference>
        <sum>
          <prod>
            <sum>
              <prod>
                <property>/instrumentation/nav[1]/radials/selected-deg</property>
                <abs>
                  <difference>
                    <property>/instrumentation/hsi[0]/dev-nav2</property>
                    <value>1</value>
                  </difference>
                </abs>
              </prod>
              <prod>
                <property>/instrumentation/nav[2]/radials/selected-deg</property>
                <property>/instrumentation/hsi[0]/dev-nav2</property>
              </prod>
            </sum>
            <abs>
              <difference>
                <property>/instrumentation/hsi[0]/ins-source</property>
                <value>1</value>
              </difference>
            </abs>
          </prod>
          <prod>
            <property>/autopilot/settings/true-heading-deg</property>
            <property>/instrumentation/hsi[0]/ins-source</property>
          </prod>
        </sum>
        <property>/instrumentation/hsi[0]/heading-deg</property>
      </difference>
    </expression>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>-0.0125</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>translate</type>
    <object-name>LocNeedle</object-name>
    <expression>
      <prod>
        <sum>
          <prod>
            <abs>
              <difference>
                <property>/instrumentation/hsi[0]/dev-nav2</property>
                <value>1</value>
              </difference>
            </abs>
            <property>/instrumentation/nav[1]/in-range</property>
            <property>/instrumentation/nav[1]/heading-needle-deflection-norm</property>
            <abs>
              <difference>
                <property>/instrumentation/hsi[0]/ins-source</property>
                <value>1</value>
              </difference>
            </abs>
          </prod>
          <prod>
            <property>/instrumentation/hsi[0]/dev-nav2</property>
            <property>/instrumentation/nav[2]/in-range</property>
            <property>/instrumentation/nav[2]/heading-needle-deflection-norm</property>
            <abs>
              <difference>
                <property>/instrumentation/hsi[0]/ins-source</property>
                <value>1</value>
              </difference>
            </abs>
          </prod>
          <max>
            <min>
              <prod>
                <property>/autopilot/internal/true-heading-error-deg</property>
                <property>/instrumentation/hsi[0]/ins-source</property>
                <value>0.1</value>
              </prod>
              <value>1</value>
            </min>
            <value>-1</value>
          </max>
        </sum>
        <value>0.016</value>
      </prod>
    </expression>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>translate</type>
    <object-name>GSArrow</object-name>
    <expression>
      <prod>
        <sum>
          <prod>
            <abs>
              <difference>
                <property>/instrumentation/hsi[0]/dev-nav2</property>
                <value>1</value>
              </difference>
            </abs>
            <property>/instrumentation/nav[1]/in-range</property>
            <property>/instrumentation/nav[1]/gs-needle-deflection-norm</property>
            <abs>
              <difference>
                <property>/instrumentation/hsi[0]/ins-source</property>
                <value>1</value>
              </difference>
            </abs>
          </prod>
          <prod>
            <property>/instrumentation/hsi[0]/dev-nav2</property>
            <property>/instrumentation/nav[2]/in-range</property>
            <property>/instrumentation/nav[2]/gs-needle-deflection-norm</property>
            <abs>
              <difference>
                <property>/instrumentation/hsi[0]/ins-source</property>
                <value>1</value>
              </difference>
            </abs>
          </prod>
        </sum>
        <value>0.02</value>
      </prod>
    </expression>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>1</z>
    </axis>
  </animation>
  <animation>
    <type>select</type>
    <object-name>ToFlag</object-name>
    <condition>
      <and>
        <or>
          <and>
            <not>
              <property>/instrumentation/hsi[0]/dev-nav2</property>
            </not>
            <property>/instrumentation/nav[1]/in-range</property>
            <property>/instrumentation/nav[1]/to-flag</property>
          </and>
          <and>
            <property>/instrumentation/hsi[0]/dev-nav2</property>
            <property>/instrumentation/nav[2]/in-range</property>
            <property>/instrumentation/nav[2]/to-flag</property>
          </and>
        </or>
        <not>
          <property>/instrumentation/hsi[0]/ins-source</property>
        </not>
      </and>
    </condition>
  </animation>
  <animation>
    <type>select</type>
    <object-name>FromFlag</object-name>
    <condition>
      <and>
        <or>
          <and>
            <not>
              <property>/instrumentation/hsi[0]/dev-nav2</property>
            </not>
            <property>/instrumentation/nav[1]/in-range</property>
            <property>/instrumentation/nav[1]/from-flag</property>
          </and>
          <and>
            <property>/instrumentation/hsi[0]/dev-nav2</property>
            <property>/instrumentation/nav[2]/in-range</property>
            <property>/instrumentation/nav[2]/from-flag</property>
          </and>
        </or>
        <not>
          <property>/instrumentation/hsi[0]/ins-source</property>
        </not>
      </and>
    </condition>
  </animation>
  <animation>
    <type>textranslate</type>
    <object-name>TrueMag</object-name>
    <factor>.5</factor>
    <property>/instrumentation/hsi[0]/ins-source</property>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <object-name>Miles1</object-name>
    <type>select</type>
    <condition>
      <greater-than>
        <property>/autopilot/route-manager/wp/dist</property>
        <value>1000</value>
      </greater-than>
    </condition>
  </animation>
  <animation>
    <object-name>Miles2</object-name>
    <type>select</type>
    <condition>
      <greater-than>
        <property>/autopilot/route-manager/wp/dist</property>
        <value>100</value>
      </greater-than>
    </condition>
  </animation>
  <animation>
    <object-name>Miles3</object-name>
    <type>select</type>
    <condition>
      <greater-than>
        <property>/autopilot/route-manager/wp/dist</property>
        <value>10</value>
      </greater-than>
    </condition>
  </animation>
  <animation>
    <object-name>Miles4</object-name>
    <type>select</type>
    <condition>
      <greater-than>
        <property>/autopilot/route-manager/wp/dist</property>
        <value>1</value>
      </greater-than>
    </condition>
  </animation>
  <animation>
    <type>textranslate</type>
    <object-name>Miles1</object-name>
    <property>/autopilot/route-manager/wp/dist</property>
    <bias>0.0001</bias>
    <factor>0.0001</factor>
    <step>1000</step>
    <axis>
      <x>1</x>
      <y>0</y>
    </axis>
  </animation>
  <animation>
    <type>textranslate</type>
    <object-name>Miles2</object-name>
    <property>/autopilot/route-manager/wp/dist</property>
    <bias>0.0001</bias>
    <factor>0.001</factor>
    <step>100</step>
    <axis>
      <x>1</x>
      <y>0</y>
    </axis>
  </animation>
  <animation>
    <type>textranslate</type>
    <object-name>Miles3</object-name>
    <property>/autopilot/route-manager/wp/dist</property>
    <bias>0.0001</bias>
    <factor>0.01</factor>
    <step>10</step>
    <axis>
      <x>1</x>
      <y>0</y>
    </axis>
  </animation>
  <animation>
    <type>textranslate</type>
    <object-name>Miles4</object-name>
    <property>/autopilot/route-manager/wp/dist</property>
    <bias>0.0001</bias>
    <factor>.1</factor>
    <step>1</step>
    <axis>
      <x>1</x>
      <y>0</y>
    </axis>
  </animation>
  <animation>
    <object-name>Speed1</object-name>
    <type>select</type>
    <condition>
      <greater-than>
        <property>/instrumentation/ins[0]/computed/ground-speed-fps</property>
        <value>1687.81</value>
      </greater-than>
    </condition>
  </animation>
  <animation>
    <object-name>Speed2</object-name>
    <type>select</type>
    <condition>
      <greater-than>
        <property>/instrumentation/ins[0]/computed/ground-speed-fps</property>
        <value>168.781</value>
      </greater-than>
    </condition>
  </animation>
  <animation>
    <object-name>Speed3</object-name>
    <type>select</type>
    <condition>
      <greater-than>
        <property>/instrumentation/ins[0]/computed/ground-speed-fps</property>
        <value>16.8781</value>
      </greater-than>
    </condition>
  </animation>
  <animation>
    <type>textranslate</type>
    <object-name>Speed1</object-name>
    <property>/instrumentation/ins[0]/computed/ground-speed-fps</property>
    <bias>0.0001</bias>
    <factor>0.0000592484</factor>
    <step>1687.80929105</step>
    <axis>
      <x>1</x>
      <y>0</y>
    </axis>
  </animation>
  <animation>
    <type>textranslate</type>
    <object-name>Speed2</object-name>
    <property>/instrumentation/ins[0]/computed/ground-speed-fps</property>
    <bias>0.0001</bias>
    <factor>0.000592484</factor>
    <step>168.780929105</step>
    <axis>
      <x>1</x>
      <y>0</y>
    </axis>
  </animation>
  <animation>
    <type>textranslate</type>
    <object-name>Speed3</object-name>
    <property>/instrumentation/ins[0]/computed/ground-speed-fps</property>
    <bias>0.0001</bias>
    <factor>0.00592484</factor>
    <step>16.8780929105</step>
    <axis>
      <x>1</x>
      <y>0</y>
    </axis>
  </animation>
  <animation>
    <type>textranslate</type>
    <object-name>Speed4</object-name>
    <property>/instrumentation/ins[0]/computed/ground-speed-fps</property>
    <bias>0.0001</bias>
    <factor>0.0592484</factor>
    <step>1.68780929105</step>
    <axis>
      <x>1</x>
      <y>0</y>
    </axis>
  </animation>
  <!--Default Lighting-->
  <animation>
    <type>material</type>
    <object-name>GsGuage</object-name>
    <object-name>TrueMag</object-name>
    <object-name>Compass</object-name>
    <object-name>LocGuage</object-name>
    <object-name>Guage</object-name>
    <emission>
      <red>0.2</red>
      <green>0.1</green>
      <blue>0.1</blue>
      <factor-prop>controls/lighting/panel-norm</factor-prop>
    </emission>
  </animation>
  <animation>
    <type>material</type>
    <object-name>Frame</object-name>
    <object-name>MilesFrame</object-name>
    <object-name>SpeedFrame</object-name>
    <emission>
      <red>0.01</red>
      <green>0.005</green>
      <blue>0.005</blue>
      <factor-prop>controls/lighting/panel-norm</factor-prop>
    </emission>
  </animation>
  <animation>
    <type>material</type>
    <object-name>CompassNeedle</object-name>
    <object-name>LocNeedle</object-name>
    <object-name>CenterPlane</object-name>
    <object-name>FromFlag</object-name>
    <object-name>ToFlag</object-name>
    <object-name>GSArrow</object-name>
    <object-name>0Ind</object-name>
    <object-name>45Ind</object-name>
    <object-name>90Ind</object-name>
    <object-name>180Ind</object-name>
    <object-name>270Ind</object-name>
    <object-name>315Ind</object-name>
    <emission>
      <red>0.1</red>
      <green>0.03</green>
      <blue>0.03</blue>
      <factor-prop>controls/lighting/panel-norm</factor-prop>
    </emission>
  </animation>
</PropertyList>
