<?xml version="1.0"?>
<PropertyList>
  <path>../Models/vsi.ac</path>
  <animation>
    <type>translate</type>
    <object-name>Current</object-name>
    <property>instrumentation/vertical-speed-indicator[0]/indicated-speed-fps</property>
    <interpolation>
      <entry>
        <ind>100</ind>
        <dep>.0736</dep>
      </entry>
      <entry>
        <ind>33.33</ind>
        <dep>.0443</dep>
      </entry>
      <entry>
        <ind>16.66</ind>
        <dep>.0297</dep>
      </entry>
      <entry><ind>0</ind><dep/>0</entry>
      <entry>
        <ind>-16.66</ind>
        <dep>-.0297</dep>
      </entry>
      <entry>
        <ind>-33.33</ind>
        <dep>-.0443</dep>
      </entry>
      <entry>
        <ind>-100</ind>
        <dep>-.0736</dep>
      </entry>
    </interpolation>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>1</z>
    </axis>
  </animation>
  <animation>
    <type>translate</type>
    <object-name>Set</object-name>
    <property>autopilot/settings/vertical-speed-fpm</property>
    <interpolation>
      <entry>
        <ind>6000</ind>
        <dep>.0736</dep>
      </entry>
      <entry>
        <ind>2000</ind>
        <dep>.0443</dep>
      </entry>
      <entry>
        <ind>1000</ind>
        <dep>.0297</dep>
      </entry>
      <entry><ind>0</ind><dep/>0</entry>
      <entry>
        <ind>-1000</ind>
        <dep>-.0297</dep>
      </entry>
      <entry>
        <ind>-2000</ind>
        <dep>-.0443</dep>
      </entry>
      <entry>
        <ind>-6000</ind>
        <dep>-.0736</dep>
      </entry>
    </interpolation>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>1</z>
    </axis>
  </animation>
  <!--Default Lighting-->
  <animation>
    <type>material</type>
    <object-name>Guage</object-name>
    <emission>
      <red>0.2</red>
      <green>0.1</green>
      <blue>0.1</blue>
      <factor-prop>controls/lighting/panel-norm</factor-prop>
    </emission>
  </animation>
  <animation>
    <type>material</type>
    <object-name>Frame</object-name>
    <emission>
      <red>0.01</red>
      <green>0.005</green>
      <blue>0.005</blue>
      <factor-prop>controls/lighting/panel-norm</factor-prop>
    </emission>
  </animation>
  <animation>
    <type>material</type>
    <object-name>Current</object-name>
    <object-name>Set</object-name>
    <emission>
      <red>0.1</red>
      <green>0.03</green>
      <blue>0.03</blue>
      <factor-prop>controls/lighting/panel-norm</factor-prop>
    </emission>
  </animation>
</PropertyList>
