<?xml version="1.0"?>
<PropertyList>
  <path>../Models/radio-altimeter.ac</path>
  <animation>
    <type>rotate</type>
    <object-name>RadioAltitude</object-name>
    <property>instrumentation/radio-altimeter[0]/indicated-altitude-ft</property>
    <interpolation>
      <entry>
        <ind>0</ind>
        <dep>0</dep>
      </entry>
      <entry>
        <ind>500</ind>
        <dep>180</dep>
      </entry>
      <entry>
        <ind>1500</ind>
        <dep>270</dep>
      </entry>
      <entry>
        <ind>2000</ind>
        <dep>292</dep>
      </entry>
      <entry>
        <ind>2500</ind>
        <dep>315</dep>
      </entry>
    </interpolation>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>RadioAltitudeBlack</object-name>
    <property>instrumentation/radio-altimeter[0]/indicated-altitude-ft</property>
    <interpolation>
      <entry>
        <ind>0</ind>
        <dep>0</dep>
      </entry>
      <entry>
        <ind>500</ind>
        <dep>180</dep>
      </entry>
      <entry>
        <ind>1500</ind>
        <dep>270</dep>
      </entry>
      <entry>
        <ind>2000</ind>
        <dep>292</dep>
      </entry>
      <entry>
        <ind>2500</ind>
        <dep>315</dep>
      </entry>
    </interpolation>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>DHAltitude</object-name>
    <property>instrumentation/radio-altimeter[0]/dial-decision-ft</property>
    <interpolation>
      <entry>
        <ind>0</ind>
        <dep>0</dep>
      </entry>
      <entry>
        <ind>500</ind>
        <dep>180</dep>
      </entry>
      <entry>
        <ind>1500</ind>
        <dep>270</dep>
      </entry>
      <entry>
        <ind>2000</ind>
        <dep>292</dep>
      </entry>
      <entry>
        <ind>2500</ind>
        <dep>315</dep>
      </entry>
    </interpolation>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>
  <!-- Adjustment wheel -->
  <animation>
    <type>pick</type>
    <object-name>DHAdjust</object-name>
    <action>
      <name>DH Reset</name>
      <button>1</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-assign</command>
        <property>instrumentation/radio-altimeter[0]/decision-height</property>
        <value>false</value>
      </binding>
    </action>
    <action>
      <name>Altimeter Adjust Up</name>
      <button>3</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-adjust</command>
        <property>instrumentation/radio-altimeter[0]/dial-decision-ft</property>
        <step>10</step>
        <min>0</min>
        <max>1000</max>
        <wrap>false</wrap>
      </binding>
    </action>
    <action>
      <name>Altimeter Adjust Down</name>
      <button>4</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-adjust</command>
        <property>instrumentation/radio-altimeter[0]/dial-decision-ft</property>
        <step>-10</step>
        <min>0</min>
        <max>1000</max>
        <wrap>false</wrap>
      </binding>
    </action>
  </animation>
  <!--Default Lighting-->
  <animation>
    <type>material</type>
    <object-name>Guage</object-name>
    <emission>
      <red>0.2</red>
      <green>0.1</green>
      <blue>0.1</blue>
      <factor-prop>controls/lighting/panel-norm</factor-prop>
    </emission>
  </animation>
  <animation>
    <type>material</type>
    <object-name>Frame</object-name>
    <emission>
      <red>0.01</red>
      <green>0.005</green>
      <blue>0.005</blue>
      <factor-prop>controls/lighting/panel-norm</factor-prop>
    </emission>
  </animation>
  <animation>
    <type>material</type>
    <object-name>RadioAltitude</object-name>
    <object-name>DHAdjust</object-name>
    <object-name>DHAltitude</object-name>
    <emission>
      <red>0.1</red>
      <green>0.03</green>
      <blue>0.03</blue>
      <factor-prop>controls/lighting/panel-norm</factor-prop>
    </emission>
  </animation>
</PropertyList>
