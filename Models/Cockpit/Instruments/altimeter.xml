  <!-- Adjustment wheel -->
  <animation>
    <type>pick</type>
    <object-name>Adjust</object-name>
    <action>
      <name>Altimeter Adjust Reset</name>
      <button>1</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-assign</command>
        <property>instrumentation/altimeter-adc[0]/setting-inhg</property>
        <value>29.92</value>
      </binding>
    </action>
    <action>
      <name>Altimeter Adjust Up</name>
      <button>3</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-adjust</command>
        <property>instrumentation/altimeter-adc[0]/setting-inhg</property>
        <step>.01</step>
        <min>0</min>
        <max>100</max>
