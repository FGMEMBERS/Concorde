<?xml version="1.0"?>
<PropertyList>
  <path>../Models/machmeter.ac</path>
  <animation>
    <type>rotate</type>
    <object-name>MachNeedle</object-name>
    <property>instrumentation/mach-indicator[0]/indicated-mach</property>
    <interpolation>
      <entry>
        <ind>.4</ind>
        <dep>-17.5</dep>
      </entry>
      <entry>
        <ind>2.4</ind>
        <dep>270</dep>
      </entry>
    </interpolation>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>LimitNeedle</object-name>
    <property>instrumentation/mach-indicator[0]/mmo-mach</property>
    <interpolation>
      <entry>
        <ind>.4</ind>
        <dep>-17.5</dep>
      </entry>
      <entry>
        <ind>2.4</ind>
        <dep>270</dep>
      </entry>
    </interpolation>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>AFT</object-name>
    <property>instrumentation/mach-indicator[0]/min</property>
    <interpolation>
      <entry>
        <ind>.4</ind>
        <dep>-17.5</dep>
      </entry>
      <entry>
        <ind>2.4</ind>
        <dep>270</dep>
      </entry>
    </interpolation>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>FWD</object-name>
    <property>instrumentation/mach-indicator[0]/max</property>
    <interpolation>
      <entry>
        <ind>.4</ind>
        <dep>-17.5</dep>
      </entry>
      <entry>
        <ind>2.4</ind>
        <dep>270</dep>
      </entry>
    </interpolation>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>Number1</object-name>
    <expression>
      <prod>
        <sum>
          <floor>
            <property>instrumentation/mach-indicator[0]/indicated-mach</property>
          </floor>
          <max>
            <prod>
              <difference>
                <property>instrumentation/mach-indicator[0]/indicated-mach</property>
                <value>0.99</value>
                <floor>
                  <property>instrumentation/mach-indicator[0]/indicated-mach</property>
                </floor>
              </difference>
              <value>100</value>
            </prod>
            <value>0</value>
          </max>
        </sum>
        <value>36</value>
      </prod>
    </expression>
    <center>
      <x-m>-0.008777</x-m>
      <y-m>-0.017</y-m>
      <z-m>0.007647</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>Number2</object-name>
    <!-- <property>instrumentation/mach-indicator[0]/indicated-mach</property> -->
    <expression>
      <prod>
        <sum>
          <div>
            <floor>
              <prod>
                <property>instrumentation/mach-indicator[0]/indicated-mach</property>
                <value>10</value>
              </prod>
            </floor>
            <value>10</value>
          </div>
          <max>
            <prod>
              <difference>
                <property>instrumentation/mach-indicator[0]/indicated-mach</property>
                <value>0.09</value>
                <div>
                  <floor>
                    <prod>
                      <property>instrumentation/mach-indicator[0]/indicated-mach</property>
                      <value>10</value>
                    </prod>
                  </floor>
                  <value>10</value>
                </div>
              </difference>
              <value>10</value>
            </prod>
            <value>0</value>
          </max>
        </sum>
        <value>360</value>
      </prod>
    </expression>
    <center>
      <x-m>-0.008777</x-m>
      <y-m>-0.009083</y-m>
      <z-m>0.007647</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>Number3</object-name>
    <property>instrumentation/mach-indicator[0]/indicated-mach</property>
    <factor>3600</factor>
    <center>
      <x-m>-0.007806</x-m>
      <y-m>-0.00409</y-m>
      <z-m>0.007647</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>
</PropertyList>
