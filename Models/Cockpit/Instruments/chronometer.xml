<?xml version="1.0"?>
<PropertyList>
  <path>../Models/chronometer.ac</path>
  <!--LEFT SIDE-->
  <animation>
    <type>textranslate</type>
    <object-name>Hour1</object-name>
    <property>instrumentation/clock[0]/indicated-hour</property>
    <factor>.01</factor>
    <step>10</step>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>textranslate</type>
    <object-name>Hour2</object-name>
    <property>instrumentation/clock[0]/indicated-hour</property>
    <factor>.1</factor>
    <step>1</step>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>textranslate</type>
    <object-name>Min1</object-name>
    <property>instrumentation/clock[0]/indicated-min</property>
    <factor>.01</factor>
    <step>10</step>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>textranslate</type>
    <object-name>Min2</object-name>
    <property>instrumentation/clock[0]/indicated-min</property>
    <factor>.1</factor>
    <step>1</step>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>textranslate</type>
    <object-name>Sec1</object-name>
    <property>sim/time/utc/second</property>
    <factor>.01</factor>
    <step>10</step>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>textranslate</type>
    <object-name>Sec2</object-name>
    <property>sim/time/utc/second</property>
    <factor>.1</factor>
    <step>1</step>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>
</PropertyList>
